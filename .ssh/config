# Note: This has to be hard-linked, because ssh doesn't support symlinks.

Host vvv-personal vvv-wme
	HostName 192.168.50.4
	User vagrant
	Port 22
	UserKnownHostsFile /dev/null
	StrictHostKeyChecking no
	PasswordAuthentication no
	ForwardAgent yes

Host vvv-personal
	IdentityFile ~/vhosts/virtual-machines/vvv-personal/.vagrant/machines/default/virtualbox/private_key

Host vvv-wme
	IdentityFile ~/vhosts/virtual-machines/vvv-wme/.vagrant/machines/default/virtualbox/private_key

