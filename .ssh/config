# TODO figure out why symlinks don't work for this. Until then, have to manually copy this to ~/.ssh/config after making changes

Host *
	ServerAliveInterval 60
	UseRoaming no
		# See http://undeadly.org/cgi?action=article&sid=20160114142733

Host vvv-personal vvv-wme
	HostName 192.168.50.4
	User vagrant
	Port 22
	UserKnownHostsFile /dev/null
	StrictHostKeyChecking no
	PasswordAuthentication no
	IdentitiesOnly yes
	ForwardAgent yes

Host vvv-personal
	IdentityFile /Users/ian/vhosts/virtual-machines/vvv-personal/.vagrant/machines/default/virtualbox/private_key

Host vvv-wme
	IdentityFile /Users/ian/vhosts/virtual-machines/vvv-wme/.vagrant/machines/default/virtualbox/private_key

Host iandunn.name
	Hostname 205.186.187.77
	User iandunn.name

Host wordcamp.org
	Hostname wordcamp.org
	User wordcamp
	ProxyCommand ssh -W %h:%p -N -l iandunn proxy.automattic.com

Host support.wordcamp.org
	Hostname support.wordcamp.org
	User wordcamp

Host wordpress.org
	Hostname iandunn.dev.wordpress.org
	User wporgdev

Host wordpress.com
	Hostname iandunn.dev.dfw.wordpress.com
	User wpdev

Host carlybish.com
	Hostname wp543664.dreamhostps.com
	User wp_x7nhq7

Host level-3-media.com
	Hostname app02.mpangohosting.com
	User level3

Host ec2-tor-relay
	Hostname ec2-54-152-233-50.compute-1.amazonaws.com
	User ubuntu
	IdentityFile ~/.ssh/tor-cloud-servers.pem
