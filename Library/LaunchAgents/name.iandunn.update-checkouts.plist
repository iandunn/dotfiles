	  <key>RunAtLoad</key>
	  <true />

	  <key>StandardErrorPath</key>
	  <string>/tmp/name.iandunn.update-checkouts-errors.log</string>

	  <key>StandardOutPath</key>
	  <string>/tmp/name.iandunn.update-checkouts-messages.log</string>
	</dict>
